{
  "name": "@pangenerator/utils",
  "version": "2.1.1",
  "description": "A collection of snippets for creative coding",
  "main": "utils.js",
  "scripts": {
    "rollup": "rollup src/$npm_package_main --file $npm_package_main --plugin rollup-plugin-terser --format umd --name \"utils\"",
    "docs": "jsdoc2md --template src/README.hbs --files src/$npm_package_main --separators > README.md",
    "build": "npm run rollup && npm run docs",
    "pretest": "npm run build",
    "test": "mocha"
  },
  "files": [
    "utils.js"
  ],
  "private": false,
  "keywords": [
    "util",
    "tool",
    "snippet"
  ],
  "author": "Krzysztof Golinski <kgolinski@pangenerator.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panGenerator/utils.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.1.2",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2"
  }
}
